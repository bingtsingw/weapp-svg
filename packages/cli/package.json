{
  "name": "@wesvg/cli",
  "version": "0.0.2",
  "private": false,
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "wesvg": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -b && cp -R ./src/templates ./dist/templates",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "rimraf oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\""
  },
  "dependencies": {
    "@oclif/core": "^2.11.8",
    "@oclif/plugin-help": "^5.2.16",
    "axios": "^1.4.0",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "xml2js": "^0.6.2",
    "zod": "^3.21.4",
    "zod-validation-error": "^1.3.1"
  },
  "devDependencies": {
    "@oclif/test": "^2.4.4",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/xml2js": "^0.4.11",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "oclif": "^3.11.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "oclif": {
    "bin": "wesvg",
    "dirname": "wesvg",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
